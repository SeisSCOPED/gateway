{
    "id": "<id>-scoped-mpass",
    "version": "0.1",
    "description": "SCOPED mpass application.",
    "jobType": "BATCH",
    "runtime": "SINGULARITY",
    "runtimeOptions": ["SINGULARITY_RUN"],
    "containerImage": "docker://ghcr.io/mspass-team/mspass:latest",
    "jobAttributes": {
        "parameterSet": {
            "archiveFilter": { 
                "includeLaunchFiles": false 
            }
        },
        "memoryMB": 1,
        "nodeCount": 1,
        "coresPerNode": 1,
        "maxMinutes": 10
    }
}